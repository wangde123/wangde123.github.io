#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/cjs/src/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/cjs/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/cjs/src/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/cjs/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/dist/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules/rimraf/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/rimraf@5.0.1/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/src/bin.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/src/bin.js" "$@"
fi
