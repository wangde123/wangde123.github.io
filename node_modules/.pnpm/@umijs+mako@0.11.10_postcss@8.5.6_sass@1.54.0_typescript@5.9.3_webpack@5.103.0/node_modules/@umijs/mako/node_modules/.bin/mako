#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/mako/bin/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/mako/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/mako/bin/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/mako/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules/@umijs/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/@umijs+mako@0.11.10_postcss@8.5.6_sass@1.54.0_typescript@5.9.3_webpack@5.103.0/node_modules:/Users/mi/Desktop/moblie/DAY-Mobile/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/mako.js" "$@"
else
  exec node  "$basedir/../../bin/mako.js" "$@"
fi
