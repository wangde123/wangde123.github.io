#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/sort-package-json@2.4.1/node_modules/sort-package-json/node_modules:/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/sort-package-json@2.4.1/node_modules:/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/sort-package-json@2.4.1/node_modules/sort-package-json/node_modules:/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/sort-package-json@2.4.1/node_modules:/Users/mi/Desktop/moblie/wangde123.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../sort-package-json/cli.js" "$@"
else
  exec node  "$basedir/../../../sort-package-json/cli.js" "$@"
fi
